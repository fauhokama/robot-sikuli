{
  "name": "robot-sikuli",
  "displayName": "robot-sikuli",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:robotSikuli.openPreview"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "robotSikuli.openPreview",
        "title": "Open Preview"
      }
    ],
    "configuration": {
      "title": "Robot Sikuli",
      "properties": {
        "robotSikuli.operativeSystem": {
          "type": "string",
          "description": "Look for images in Darwin / Windows / Linux folder."
        },
        "robotSikuli.oneImg": {
          "type": "array",
          "description": "Keywords to trigger a one image display",
          "default": [
            "Click",
            "Wait Until Screen Contain",
            "Wait Until Screen Not Contain",
            "Double Click"
          ]
        },
        "robotSikuli.twoImg": {
          "type": "array",
          "description": "Keywords to trigger a two image display",
          "default": [
            "Click In",
            "Double Click In"
          ]
        }
      }
    }
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.72.0",
    "@vscode/test-electron": "^2.1.5",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@types/glob": "^8.0.0",
    "glob": "^8.0.3"
  }
}