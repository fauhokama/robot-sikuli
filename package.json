{
  "name": "robot-sikuli",
  "displayName": "robot-sikuli",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:robotSikuli.openPreview"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "robotSikuli.openPreview",
        "title": "Open Preview"
      }
    ],
    "configuration": {
      "title": "Robot Sikuli",
      "properties": {
        "robotSikuli.operativeSystem": {
          "type": "string",
          "description": "Look for images in Darwin / Windows / Linux folder."
        }
      }
    }
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.72.0",
    "@types/node": "16.x",
    "typescript": "^4.8.4",
    "@vscode/test-electron": "^2.1.5"
  }
}